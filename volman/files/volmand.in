#!/bin/sh

# $FreeBSD: branches/RELENG_9_1_0/sysutils/volman/files/volmand.in 300897 2012-07-14 14:29:18Z beat $
#
# PROVIDE: volmand
# REQUIRE: LOGIN
# KEYWORD: shutdown nojail

. /etc/rc.subr

name="volmand"
rcvar=volmand_enable

load_rc_config $name

: ${volmand_enable:="NO"}

command="%%PREFIX%%/sbin/volmand"
pidfile="/var/run/volmand.pid"

run_rc_command "$1"
