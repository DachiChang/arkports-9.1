<?xml version="1.0" encoding="UTF-8"?>
<openbox_menu xmlns="http://openbox.org/3.4/menu">
    <menu id="system-menu" label="System">
        <item label="Reconfigure">
            <action name="Reconfigure" />
        </item>
        <item label="Logout">
            <action name="Execute">
                <command></command>
            </action>
        </item>
        <item label="Reboot">
            <action name="Execute">
                <command></command>
            </action>
        </item>
        <item label="Shutdown">
            <action name="Execute">
                <command></command>
            </action>
        </item>
    </menu>
    <menu id="desktop-menu" label="Desktop">
        <item label="TakeShot">
            <action name="Reconfigure" />
        </item>
        <item label="ScreenLock">
            <action name="Execute">
                <command>xtrlock</command>
            </action>
        </item>
    </menu>
    <menu id="favorite-menu" label="Favorite">
        <item label="Firefox">
            <action name="Execute">
                <command>firefox</command>
                <startupnotify>
                    <enabled>true</enabled>
                </startupnotify>
            </action>
        </item>
        <item label="Pcmanfm">
            <action name="Execute">
                <command>pcmanfm</command>
                <startupnotify>
                    <enabled>true</enabled>
                </startupnotify>
            </action>
        </item>
        <item label="Filezilla">
            <action name="Execute">
                <command>filezilla</command>
                <startupnotify>
                    <enabled>true</enabled>
                </startupnotify>
            </action>
        </item>
        <item label="DeadBeef">
            <action name="Execute">
                <command>deadbeef</command>
                <startupnotify>
                    <enabled>true</enabled>
                </startupnotify>
            </action>
        </item>
        <item label="Gedit">
            <action name="Execute">
                <command>gedit</command>
                <startupnotify>
                    <enabled>true</enabled>
                </startupnotify>
            </action>
        </item>
        <item label="Terminal">
            <action name="Execute">
                <command>xterm</command>
                <startupnotify>
                    <enabled>true</enabled>
                </startupnotify>
            </action>
        </item>
    </menu>
    <menu id="root-menu" label="root-menu">
        <menu id="favorite-menu"/>
        <menu id="applications-menu" label="Applications" execute="openbox-menu -s" />
        <menu id="settings-menu" label="Settings" execute="openbox-menu -s settings.menu" />
        <menu id="system-menu"/>
    </menu>
</openbox_menu>
